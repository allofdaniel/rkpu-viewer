{
  "appId": "com.allofdaniel.rkpuviewer",
  "appName": "RKPU Viewer",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#000000",
    "useLegacyBridge": false,
    "webContentsDebuggingEnabled": true
  },
  "plugins": {
    "Keyboard": {
      "resize": "none"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#000000"
    }
  }
}
